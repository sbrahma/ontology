map:database a d2rq:Database;
	d2rq:jdbcDriver "org.postgresql.Driver";
	d2rq:jdbcDSN "jdbc:postgresql://localhost/olu";
	d2rq:username "rapw3k";
	d2rq:password "rapulin";
	.

# create instances for areas_master_h1_cz_wbn_geometry
map:areas_master_h1_cz_wbn_geometry a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "LandUse/@@areas_master_h1_cz_wbn.id@@/geometry";
	d2rq:class geo:Geometry;
	d2rq:classDefinitionLabel "Geometry";
	.
map:areas_master_h1_cz_wbn_geometry_asWKT a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn_geometry;
	d2rq:property geo:asWKT;
	d2rq:column "areas_master_h1_cz_wbn.geom";
	d2rq:datatype geo:wktLiteral;
	.


# Table areas_master_h1_cz_wbn
map:areas_master_h1_cz_wbn a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "LandUse/@@areas_master_h1_cz_wbn.id@@";
	d2rq:class olu:LandUse;
	d2rq:classDefinitionLabel "LandUse";
	.
map:areas_master_h1_cz_wbn__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property rdfs:label;
	d2rq:pattern "LandUse #@@areas_master_h1_cz_wbn.id@@";
	.
map:areas_master_h1_cz_wbn_geom a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property geo:hasGeometry;
	d2rq:uriPattern "LandUse/@@areas_master_h1_cz_wbn.id@@/geometry";
	.		
map:areas_master_h1_cz_wbn_hilucs_value a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property olu:hilucsLandUse;
	d2rq:column "areas_master_h1_cz_wbn.hilucs_value";
	d2rq:datatype xsd:decimal;
	.
map:areas_master_h1_cz_wbn_original_value a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property olu:specificLandUse;
	d2rq:column "areas_master_h1_cz_wbn.original_value";
	d2rq:datatype xsd:string;
	.
map:areas_master_h1_cz_wbn_country_code a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property olu:countryCode;
	d2rq:column "areas_master_h1_cz_wbn.country_code";
	d2rq:datatype xsd:string;
	.
map:areas_master_h1_cz_wbn_municipal_code a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property olu:municipalCode;
	d2rq:column "areas_master_h1_cz_wbn.municipal_code";
	d2rq:datatype xsd:string;
	.
map:areas_master_h1_cz_wbn_validfrom a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property prov:generatedAtTime;
	d2rq:column "areas_master_h1_cz_wbn.validfrom";
	d2rq:datatype xsd:dateTime;
	.
map:areas_master_h1_cz_wbn_validto a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property prov:invalidatedAtTime;
	d2rq:column "areas_master_h1_cz_wbn.validto";
	d2rq:datatype xsd:dateTime;
	.
	
#all rules for geometry_source			
map:areas_master_h1_cz_wbn_geometry_source a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_wbn;
	d2rq:property olu:geometrySource;
	d2rq:uriPattern "http://w3id.org/foodie/wbn/@@areas_master_h1_cz_wbn.geometry_source_gid@@";
	.
	
