# create instances for areas_master_h1_cz_clc_2012_geometry
map:areas_master_h1_cz_clc_2012_geometry a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "LandUse/@@areas_master_h1_cz_clc_2012.id@@/geometry";
	d2rq:class geo:Geometry;
	d2rq:classDefinitionLabel "Geometry";
	.
map:areas_master_h1_cz_clc_2012_geometry_asWKT a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:areas_master_h1_cz_clc_2012_geometry;
	d2rq:property geo:asWKT;
	d2rq:column "areas_master_h1_cz_clc_2012.geom";
	d2rq:datatype virtrdf:Geometry;
	.


